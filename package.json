{
  "name": "typed-local-storage",
  "version": "0.0.1",
  "description": "Type-safe wrapper around browser's localStorage API using Zod",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && tsc --module ES6 --outDir dist/esm",
    "test": "vitest",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "typescript",
    "localStorage",
    "zod",
    "type-safe"
  ],
  "author": "Marcel Soubkovsky",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcelrsoub/typed-local-storage"
  },
  "bugs": {
    "url": "https://github.com/marcelrsoub/typed-local-storage/issues"
  },
  "homepage": "https://github.com/marcelrsoub/typed-local-storage#readme",
  "devDependencies": {
    "@types/node": "^20.8.7",
    "happy-dom": "^17.4.3",
    "typescript": "^5.2.2",
    "vitest": "^3.0.8",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
